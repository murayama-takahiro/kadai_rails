<h1>つぶやき一覧</h1>
<p><%= notice %></p>
<table>
    <th>つぶやき</th>
    <% @twees.each do |twee| %>
    <tr>
        <td><%= twee.content %></td>
        <td><%= link_to "詳細確認", twee_path(twee.id) %></td>
        <td><%= link_to "編集する", edit_twee_path(twee.id) %></td>
        <td><%= link_to "削除する", twee_path(twee.id), method: :delete, data: {confirm: "本当にいいですか？"} %></td>
    </tr>
    <% end %>
</table>
<%= link_to "新規投稿する", new_twee_path %>
